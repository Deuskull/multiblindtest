{
  "name": "multiblindtest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"npm run front/serve\" \"npm run server/watch\"",
    "build": "npm run front/build && npm run server/build",
    "front/serve": "vue-cli-service serve",
    "front/build": "vue-cli-service build",
    "server/watch": "tsc -w -p ./tsconfig_server.json",
    "server/build": "tsc -p ./tsconfig_server.json"
  },
  "dependencies": {
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.17.1",
    "express-serve-static-core": "^0.1.1",
    "glob": "^7.1.6",
    "http": "0.0.0",
    "uuid": "^8.3.0",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.5",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.4.3",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@types/sockjs": "^0.3.32",
    "@types/universal-analytics": "^0.4.4",
    "@vue/cli-plugin-router": "^4.5.6",
    "@vue/cli-plugin-typescript": "^4.5.6",
    "@vue/cli-plugin-vuex": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "concurrently": "^5.3.0",
    "fuse.js": "^6.4.1",
    "gsap": "^3.5.1",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "typescript": "~4.0.2",
    "universal-analytics": "^0.4.23",
    "vue-i18n": "^8.21.1",
    "vue-template-compiler": "^2.6.12"
  }
}
